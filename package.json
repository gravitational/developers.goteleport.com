{
  "name": "teleport-docs-next",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "git-update": "git submodule update --remote",
    "build-node": "rm -rf .build && tsc --build tsconfig.node.json && tsc-esm-fix --target='.build' --ext='.mjs'",
    "add-symlinks": "node ./scripts/add-symlinks.mjs",
    "generate-sitemap": "node ./scripts/generate-sitemap.mjs",
    "generate-article-links": "node ./scripts/generate-article-links.mjs",
    "preflight": "yarn build-node && yarn add-symlinks && yarn generate-article-links",
    "dev": "yarn preflight && next dev",
    "build": "yarn preflight && next build",
    "update-and-build": "yarn git-update && yarn build && yarn generate-sitemap",
    "start": "next start",
    "test": "loadr -- uvu",
    "base:eslint": "eslint --cache --cache-location node_modules/.cache/eslint '**/*.{js,jsx,ts,tsx}'",
    "base:prettier": "prettier '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "yarn base:eslint --fix && yarn base:prettier --write -l",
    "lint-check": "yarn base:eslint && yarn base:prettier --check",
    "typecheck": "tsc --noEmit --skipLibCheck --incremental --tsBuildInfoFile node_modules/.cache/tsc/tsbuildinfo --project .",
    "markdown-lint": "remark 'content/**/docs/pages/**/*.mdx' --frail",
    "markdown-lint-external-links": "WITH_EXTERNAL_LINKS=true yarn markdown-lint",
    "markdown-fix": "FIX=true yarn markdown-lint -o"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --check"
    ],
    "*.{json}": "prettier --check"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "^1.6.2",
    "@algolia/autocomplete-theme-classic": "^1.6.2",
    "@algolia/client-search": "^4.13.0",
    "@mdx-js/mdx": "^2.1.1",
    "@mdx-js/react": "^2.1.1",
    "@reach/listbox": "^0.16.2",
    "algoliasearch": "^4.13.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.27.0",
    "next": "13.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-use": "^17.3.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "^2.1.1",
    "@next/bundle-analyzer": "^12.1.0",
    "@svgr/webpack": "^6.2.1",
    "@types/estree-jsx": "^0.0.1",
    "@types/glob": "^7.2.0",
    "@types/hast": "^2.3.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.27",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "acorn": "^8.7.1",
    "acorn-jsx": "^5.3.2",
    "acorn-walk": "^8.2.0",
    "ajv": "^8.11.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.0.8",
    "glob": "^8.0.1",
    "got": "12.1.0",
    "gray-matter": "^4.0.3",
    "hast-util-heading-rank": "^2.1.0",
    "hast-util-to-string": "^2.0.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.4.1",
    "loadr": "^0.1.1",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-frontmatter": "^1.0.0",
    "mdast-util-gfm": "^2.0.1",
    "mdast-util-mdx": "^2.0.0",
    "micromark-extension-frontmatter": "^1.0.0",
    "micromark-extension-gfm": "2.0.1",
    "micromark-extension-mdxjs": "^1.0.0",
    "postcss": "^8.4.31",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.4",
    "prettier": "^2.6.2",
    "prettier-config-standard": "^5.0.0",
    "probe-image-size": "^7.2.2",
    "rehype-highlight": "^5.0.2",
    "rehype-slug": "^5.0.1",
    "remark-cli": "10.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-lint-no-dead-urls": "^1.1.0",
    "remark-mdx": "^2.1.1",
    "remark-mdx-disable-explicit-jsx": "^0.1.0",
    "remark-preset-lint-markdown-style-guide": "^5.1.2",
    "remark-validate-links": "^11.0.2",
    "sharp": "^0.30.5",
    "simple-git-hooks": "^2.7.0",
    "stringify-object": "^4.0.1",
    "stylelint": "^14.8.0",
    "stylelint-config-standard": "^25.0.0",
    "to-vfile": "7.2.3",
    "tsc-esm-fix": "^2.13.0",
    "tsm": "^2.2.1",
    "typescript": "^4.6.3",
    "unified": "^10.1.2",
    "unified-lint-rule": "^2.1.1",
    "unist-util-find": "^1.0.2",
    "unist-util-visit": "4.1.0",
    "unist-util-visit-parents": "^5.1.0",
    "uvu": "^0.5.3",
    "vfile": "^5.3.2",
    "webpack": "^5.76.0"
  }
}
